  margin-top:5px;
}
.left159 .leftNav .com a{
  font-size:14px;
  font-weight:bold;
  color: #004F98;
  float:left;
  line-height:36px;
  text-decoration:underline;
}
.left159 .leftNav .com a:hover{
  text-decoration:none;
}
.left159 .leftNav .nav1 span{
  background:url(../img/icon.gif) no-repeat -45px -556px;
}
.left159 .leftNav .nav2 span{
  background:url(../img/icon.gif) no-repeat -92px -556px;
}
.left159 .leftNav .nav3 span{
  background:url(../img/icon.gif) no-repeat -319px -556px;
}
.left159 .leftNav .nav4 span{
  background:url(../img/icon.gif) no-repeat -276px -592px;
}
.left159 .leftNav .nav5 span{
