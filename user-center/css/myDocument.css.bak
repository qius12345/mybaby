#navibar{
  width:1000px;
  height:30px;
  margin:0 auto;
}
#navibar .navibar-wap{
  font-family:"Œ¢»Ì—≈∫⁄";
  position:relative;
}
#navibar .navibar-sites{
  line-height:30px;
}
#navibar dd{
  float:left;
}
#navibar dt{
  float:left;
}
#navibar .navibar-lb{
  float:right;
  line-height:30px;
}

.wrap{
  width:1000px;
  margin:0 auto;
}
.nav{
  background:url(../img/02.png) no-repeat;
  width:1000px;
  height:50px;
  border-bottom:4px solid #d1d1d1;
/*  position:relative;*/
}
.nav .logo{
  background:url(../img/snslogo.gif) no-repeat;
  width:118px;
  height:50px;
  margin-left:15px;
  float:left;
}
.nav .logo a{
  display:block;
  width:118px;
  height:50px;
  text-indent:-9999px;
}
.nav .navText{
  float:left;
  height:50px;
  line-height:50px;
  margin-left:15px;
}
.nav .navText a{
  color:#fff;
  font-size:14px;
  margin-left:20px;
}
.nav .navText a:hover{
  color:red;
  text-decoration:underline;
}

#mainSearchFormId{
  float:right;
  width:350px;
  margin-top:15px;
}
#mainSearchFormId .myName{
  float:left;
  width:142px;
  height:25px;
  background:url(../img/admin.gif) no-repeat -858px -510px;
  overflow:hidden;
  margin-right:5px;
}
#mainSearchFormId .myName input{
  margin-top:2px;margin-left:4px;
  padding:0;
  width:135px;
  height:18px;
  border:none;
  color: #919ca2;
}
#sel{
  float: left;
  width: 87px;
  height: 24px;
}
#sel-info{
  background:url(../img/admin.gif) no-repeat -913px -434px;
  width: 87px;
  height: 23px;
  line-height:23px;
  padding-left:10px;
  cursor: pointer;
  color: #919ca2;
}
#mainSearchFormId .simulation .go{
  border:none;
  float:left;
  background:url(../img/admin.gif) no-repeat -934px -39px;
  width:66px;
  height:27px;
  margin-left:10px;
  cursor: pointer;
}


.mainer{
  width:1000px;
}
.left159{
  width:160px;
  height:600px;
  float:left;
  background:#f5f5f5;
  margin:0;
  padding:0;
}
.left159 .leftNav{
  padding-left:10px;
  padding-top:10px;
}
.left159 .leftNav li{
  height:36px;
  margin-top:20px;
}
.left159 .leftNav .com span{  
  width:36px;
  height:30px;
